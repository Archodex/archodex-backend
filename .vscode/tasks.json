{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo build"
		},
		{
			"type": "cargo",
			"command": "lambda",
			"args": [
				"build",
				"--release",
				"--arm64",
				"--package",
				"lambda",
				"--output-format",
				"zip"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true,
			},
			"label": "rust: cargo lambda build"
		},
		{
			"type": "process",
			"command": "act",
			"args": [
				"--var",
				"ARCHODEX_SOURCE_DIR=${workspaceFolder}",
				"--platform",
				"ubuntu-latest=-self-hosted",
				"--env",
				"AWS_PROFILE=archodex-dev-AdministratorAccess",
				"--job",
				"deploy",
			],
			"problemMatcher": [],
			"group": {
				"kind": "build",
			},
			"label": "act: Run GitHub Actions Workflow deploy.yml locally"
		}
	]
}