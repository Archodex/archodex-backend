{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo build"
		},
		{
			"type": "cargo",
			"command": "lambda",
			"args": [
				"build",
				"--release",
				"--arm64",
				"--package",
				"lambda",
				"--output-format",
				"zip"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true,
			},
			"label": "rust: cargo lambda build"
		}
	]
}