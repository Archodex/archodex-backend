{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug local backend server",
      "cargo": {
        "args": [
          "build",
          "--bin=server",
          "--package=server"
        ],
        "filter": {
          "name": "server",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "env": {
        "COLORTERM": "truecolor",
        "ENDPOINT": "http://localhost:5732",
        "RUST_BACKTRACE": "1",
        // Uncomment and modify to set a different log filter
        // (https://docs.rs/tracing-subscriber/latest/tracing_subscriber/filter/struct.EnvFilter.html#directives)
        //"RUST_LOG": "info",
        // The following must be set in your VS Code user settings
        "SURREALDB_URL": "${config:archodex.SURREALDB_URL}",
        // The following are required unless you are using anonymous access with a local SurrealDB service
        "SURREALDB_USERNAME": "${config:archodex.SURREALDB_USERNAME}",
        "SURREALDB_PASSWORD": "${config:archodex.SURREALDB_PASSWORD}",
        // Set LOCAL_FRONTEND to "true" to use a local development archodex-frontend environment
        "LOCAL_FRONTEND": "${config:archodex.LOCAL_FRONTEND}",
        // The following are optional values that are typically only used by Archodex.com developers
        "ARCHODEX_DOMAIN": "${config:archodex.ARCHODEX_DOMAIN}",
        "COGNITO_USER_POOL_ID": "${config:archodex.COGNITO_USER_POOL_ID}",
        "COGNITO_CLIENT_ID": "${config:archodex.COGNITO_CLIENT_ID}",
      },
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug archodex.com managed backend server",
      "cargo": {
        "args": [
          "build",
          "--bin=server",
          "--package=server",
          "--features=archodex-com",
          "${config:archodex.ARCHODEX_COM_ADDITIONAL_BUILD_FLAG}"
        ],
        "filter": {
          "name": "server",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "env": {
        "COLORTERM": "truecolor",
        "ENDPOINT": "http://localhost:5731",
        "RUST_BACKTRACE": "1",
        // Uncomment and modify to set a different log filter
        // (https://docs.rs/tracing-subscriber/latest/tracing_subscriber/filter/struct.EnvFilter.html#directives)
        //"RUST_LOG": "info",
        // The following must be set in your VS Code user settings
        "ACCOUNTS_SURREALDB_URL": "${config:archodex.ARCHODEX_COM_SURREALDB_URL}",
        "AWS_PROFILE": "${config:archodex.ARCHODEX_COM_AWS_PROFILE}",
        "CUSTOMER_DATA_OU_ID": "${config:archodex.ARCHODEX_COM_CUSTOMER_DATA_OU_ID}",
        // Set LOCAL_FRONTEND to "true" to use a local development archodex-frontend environment
        "LOCAL_FRONTEND": "${config:archodex.LOCAL_FRONTEND}",
        // Optionally set the following values in your VS Code user settings as needed
        "ARCHODEX_DOMAIN": "${config:archodex.ARCHODEX_DOMAIN}",
        "COGNITO_USER_POOL_ID": "${config:archodex.COGNITO_USER_POOL_ID}",
        "COGNITO_CLIENT_ID": "${config:archodex.COGNITO_CLIENT_ID}",
        // Optionally set the following to "local" for local development mode
        "ARCHODEX_DEV_MODE": "${config:archodex.ARCHODEX_COM_DEV_MODE}",
      },
    }
  ]
}