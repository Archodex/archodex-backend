  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'server'",
      "cargo": {
        "args": [
          "build",
          "--bin=server",
          "--package=server"
        ],
        "filter": {
          "name": "server",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "env": {
        "AWS_PROFILE": "ddbtest",
        "COLORTERM": "truecolor",
      }
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'server'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=server",
          "--package=server"
        ],
        "filter": {
          "name": "server",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'archodex-backend'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=archodex-backend"
        ],
        "filter": {
          "name": "archodex-backend",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
